@{
}

@model List<StudentPortal.Models.Entities.Student>

<body>
    <table>
        <caption>Students</caption>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
        </tr>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.Id</td>
                <td>@student.Name</td>
                <td>@student.Email</td>
                <td>
                    <a>Edit</a>
                </td>
            </tr>
        }
    </table>
</body>



